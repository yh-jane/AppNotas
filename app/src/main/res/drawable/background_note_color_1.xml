<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="2.00dp"
                android:color="@color/colorDefaultNoteColor"/>
        </shape>
    </item>

    <item
        android:bottom="3.00dp"
        android:left="3.00dp"
        android:right="3.00dp"
        android:top="3.00dp">

        <shape android:shape="oval">
            <stroke
                android:width="6dp"
                android:color="@color/colorMiscellaneousBackground"/>
        </shape>

    </item>

    <item
        android:bottom="6.00dp"
        android:left="6.00dp"
        android:right="6.00dp"
        android:top="6.00dp">

        <shape android:shape="oval">
            <solid
                android:color="@color/colorDefaultNoteColor"/>
        </shape>

    </item>


</layer-list>